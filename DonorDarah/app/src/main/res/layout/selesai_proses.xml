<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:gravity="center"
        android:textColor="#091dfd"
        android:text="pendaftaran berhasil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:gravity="center"
        android:textColor="#000000"
        android:text="No Registrasi :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/idReg"
        android:gravity="center"
        android:textSize="24dp"
        android:textColor="#f2747f"
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:gravity="center"
        android:textColor="#000000"
        android:text="catat No Registrasi anda dan tunjukan kepada petugas maksimal 2 jam setelah pendaftaran"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>
